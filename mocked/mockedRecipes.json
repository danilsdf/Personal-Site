      {"ingredientId": "694193652fb4d1c02187927c", "quantity": 750, "unit": "g", "grams": 750, "note": "raw (peeled, deveined)"},
      {"ingredientId": "694193652fb4d1c02187927d", "quantity": 250, "unit": "g", "grams": 250, "note": null},
