    ],
    "instructions": [
      {"section": "Prepare ingredients", "image": "", "steps": ["Rinse cherry tomatoes under cold running water.", "If shrimp are frozen, thaw fully and pat dry with a paper towel.", "Prepare salt, black pepper, and a small amount of oil or non-stick spray."], "timing": {"activeMin": 5}},
      {"section": "Cook farfalle pasta", "image": "", "steps": ["Bring a pot of salted water to a boil.", "Add farfalle pasta.", "Cook for 5 minutes (slightly undercooked / al dente).", "Drain pasta and set aside."], "timing": {"activeMin": 10}},
      {"section": "Cook shrimp", "image": "", "steps": ["Heat a pan over medium-high heat.", "Add a small amount of oil or non-stick spray.", "Add shrimp in a single layer.", "Season lightly with salt and black pepper.", "Cook 2–3 minutes per side, until shrimp turn pink and opaque.", "Remove from heat and set aside."], "timing": {"activeMin": 8}},
      {"section": "Bake pasta with feta & tomatoes", "image": "", "steps": ["Preheat oven to 200°C / 400°F.", "In a baking dish: add cooked pasta and cherry tomatoes, then place feta block in the center.", "Bake for 20–25 minutes, until tomatoes burst and feta becomes soft and lightly golden."], "timing": {"ovenC": 200, "minutes": 25}},
      {"section": "Create the sauce", "image": "", "steps": ["Remove dish from oven.", "Gently press tomatoes to release juices.", "Mix softened feta with pasta and tomatoes until creamy."], "timing": {"activeMin": 5}},
      {"section": "Combine & finish", "image": "", "steps": ["Add cooked shrimp to the pasta.", "Mix gently so shrimp stay intact.", "Taste and adjust salt and black pepper if needed.", "Portion into meal prep containers.", "Let food cool completely before closing containers."], "timing": {"activeMin": 5}}
    ],
    "mealPrep": {"fridgeDays": 5, "freezerDays": 30, "reheatNotes": "Add 1–2 tbsp water before reheating if it feels dry."},
